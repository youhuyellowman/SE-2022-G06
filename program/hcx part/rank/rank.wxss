/**index.wxss**/
page {
  width: 100%;
  height: 100%;
  background-color: beige;
}
.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: flex;
  align-items: center;
  justify-content: center;
}
/* 背景图片 */
.container .bg-img{
  position:absolute;
  width: 100%;
  height: 100%;
  z-index: 1;
  opacity: 60%;
  background-size: cover;
  background-image: url(data:image/jpeg;base64,);
}
.container .rank-bottom{
  position: fixed;
  width: 100%;
  height: 90px;
  bottom: 0px;
  background-color: #fff;
  border-radius: 30px;
  border-width: 2px;
  border-style: solid;
  border-color: #565756;
  background-image: linear-gradient(120deg, #9B9B38, #F2F2E6);
  z-index: 2;
}
.container .rank-bottom .rank-btn1{
  position: absolute;
  width: 118px;
  height: 48px;
  margin-left: 60px;
  margin-top: 20px;
  padding-left: 0px;
  padding-top: 8px;
  border-radius: 20px;
  border-width: 2px;
  border-style: solid;
  border-color: #565756;
  background: rgba(0,0,0,0);
}
.container .rank-bottom .rank-btn2{
  position: absolute;
  width: 118px;
  height: 48px;
  margin-left: 212px;
  margin-top: 20px;
  padding-left: 0px;
  padding-top: 8px;
  border-radius: 20px;
  border-width: 2px;
  border-style: solid;
  border-color: #565756;
  background: rgba(0,0,0,0);
}
.container .rank-bottom .rank-btn1 .btn-txt{
  position: relative;
  margin-left: 52px;
  margin-right: 0px;
  font-weight: 600;
  font-size: 18px;
  font-family: 方正姚体;
}
.container .rank-bottom .rank-btn1 .btn-img{
  position: absolute;
  width: 30px;
  height: 30px;
  margin-left: 20px;
}
.container .rank-bottom .rank-btn2 .btn-txt{
  position: relative;
  margin-left: 52px;
  margin-right: 0px;
  font-weight: 600;
  font-size: 18px;
  font-family: 方正姚体;
}
.container .rank-bottom .rank-btn2 .btn-img{
  position: absolute;
  width: 30px;
  height: 30px;
  margin-left: 20px;
}

