/**index.wxss**/
page {
  width: 100%;
  height: 100%;
  background-color: beige;
}
.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: flex;
  align-items: center;
  justify-content: center;
}

/* 背景图片 */
.container .bg-img{
  position:absolute;
  width: 100%;
  height: 100%;
  z-index: 1;
  opacity: 60%;
  background-size: cover;
  background-image: url(data:image/jpeg;base64,);
}

/* 输入框主体 */
.container .chgname-body{
  position: absolute;
  margin-bottom: 250px;
  width: 80%;
  height: 15%;
  border-top: 2px solid #7a7a7a;
  border-bottom: 2px solid #7a7a7a;
  align-items: center;
  justify-content: center;
}

.container .chgname-body .chgname-txt{
  position: absolute;
  width: 100%;
  margin: 10px;
  font-size: 20px;
  color: black;
  font-family: 方正姚体;
  font-weight: 600;
}
.container .chgname-body .chgname-input{
  position: absolute;
  align-items: center;
  text-align: center;
  margin-top: 50px;
  margin-left: 50px;
  width: 200px;
  height: 40px;
  border-color: #7a7a7a;
  border-radius: 20px;
  border-width: 2px;
  border-style: solid;
  font-weight: 520;
  font-size: 20px;
  font-family: 方正姚体;
  color: black;
  box-shadow: 5px 5px 5px 2px #444444;
  outline: none;
  background: rgba(0,0,0,0);
}
/* 授权按钮 */
.container .chgname-btn{
  margin-top: 80px;
  display:flex;
  width: 150px;
  height: 60px;
  border-radius: 30px;
  border-width: 2px;
  border-style: solid;
  border-color: #565756;
  text-align: center;
  align-items: center;
  justify-content: center;
  color: black;
  font-size: 24px;
  font-family: Arial;
  font-style: oblique;
  font-weight: 600;
  z-index: 2;
  box-shadow: -8px 8px 6px #565756 inset;
  background-image: url(data:image/jpeg;base64,);
}