/* pages/登录界面/login.wxss */
page {
  width: 100%;
  height: 100%;
  background-color: beige;
}
.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: relative;
}

/* 背景图片 */
.container .bg-img{
  position:absolute;
  width: 100%;
  height: 100%;
  z-index: 1;
  opacity: 60%;
}

/* LOGO图片 */
.container .logo-img{
  border-radius: 0px 40px 0px 40px;
  border-color: #292421;
  border-width: 3px;
  border-style: dashed;
  margin-top: 20%;
  width: 144px;
  height: 144px;
  z-index: 2;
}

/* 小程序标题 */
.container .login-title{
  position: absolute;
  height: 100%;
  width: 100%;
  align-items: center;
  margin-top: 280px;
  text-align: center;
  font-size: 50px;
  font-family: 华文彩云;
  font-weight: 600;
  z-index: 2;
}
.container .login-btn .btn-img{
  display: flex;
}
/* 注释信息 */
.container .login-item{
  position: relative;
  text-align: center;
  margin-bottom: 10%;
  font-size: 20px;
  font-family: Merriweather;
  font-weight: 500;
  z-index: 2;
}
/* 授权按钮 */
.container .login-btn{
  display:flex;
  width: 220px;
  height: 60px;
  border-radius: 30px;
  border-width: 2px;
  border-style: inset;
  border-color: black;
  text-align: center;
  align-items: center;
  justify-content: center;
  color: black;
  background-color: #02D668;
  font-size: 24px;
  font-family: Arial;
  font-style: oblique;
  font-weight: 600;
  z-index: 2;
  box-shadow: -8px 8px 5px #444444 inset;
  background-image: url(data:image/jpeg;base64,);
}