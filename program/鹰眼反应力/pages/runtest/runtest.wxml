<view class="container">
  <image class="img"></image>
  <button class="testbutton" bindtap="click" style="background-color:{{color1}}">开始测试</button>
  <label class="first">第一次测试</label>
  <view class="firsttxt" bindtap="firstshow">{{first}}</view>
  <view id="fenge1"></view>
  <label class="second">第二次测试</label>
  <view class="secondtxt" bindtap="secondshow">{{second}}</view>
  <view id="fenge2"></view>
  <label class="third">第三次测试</label>
  <view class="thirdtxt" bindtap="thirdshow">{{third}}</view>
  <view id="fenge3"></view>
  <button class="restart" bindtap="again">重新开始</button>
  <button class="save" bindtap="savescore">保存成绩</button>
  <view class="rank-bottom">
    <button class="rank-btn1" bindtap="torank">
      <image class="btn-img"></image>
      <text class="btn-txt">成绩</text>
    </button>
    <button class="rank-btn2" bindtap="tomy">
      <image class="btn-img"></image>
      <text class="btn-txt">我的</text>
    </button>
  </view>
</view>